.page .banner{
  width: 100%;
  height: 400rpx;
  color: #fff;
}
.col{
  display: flex;
  flex-direction: column;  
  justify-content: center;
  align-items: center;
}
.txt{
  margin-top: 20rpx
}
.page .banner .top{
  width: 100%;
  margin-bottom: 28rpx
}
.page .banner .middle .line{
  width: 40%;
  height: 1rpx;
  border-top: 2rpx dotted #fff
}
.page .banner .middle .san{
  width:25rpx;
  height: 25rpx;
}
.page .banner .bottom{
  margin-top: 40rpx;
}
.page .banner .bottom .line{
  width: 1rpx;
  height: 80rpx;
  border-left: 2rpx dotted #fff;
  margin:0 80rpx 0rpx 
}
.page .detail{
  margin-bottom:15rpx; 
  padding:30rpx; 
  color: #009c62;
  background-color: #fff;
}
.page .content .type-2 {
  align-items: center;
  border-bottom: 0;
  padding-top: 35rpx;
  padding-bottom: 35rpx;
}
.page .content .type-2 .addCard{
  display: flex;
  flex-direction: row; 
  justify-content: flex-start;
  align-items: center;
}
.page .content .add{
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx
}
.page .content .label{
  font-size: 1em;
  margin-right: 20rpx
}
.page .getMon{
  margin-top: 90rpx;
}
.page .getMon .btn{
  width: 70%;
  height: 110rpx;
}
.page .getMon .label{
  font-size: 0.8em;
  color: #888;
  margin-top: 50rpx
}
.page .banner{
  background: url(data:image/png;base64,);
