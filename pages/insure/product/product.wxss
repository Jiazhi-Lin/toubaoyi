.page .label{
  align-self: flex-start;
  padding: 20rpx 0 20rpx 38rpx;
}
.page .contents{
  flex-wrap: wrap;
  padding: 0 23rpx 0 23rpx;
  box-sizing: border-box
}
.page .contents .product-box{
  position: relative;
  width: 29%;
  height: auto;
  margin-bottom: 32rpx;
}
.page .contents .product-box .default{
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  left: 0;
  top: 0;
}
.page .contents .product-box image{
  width: 100%;
  height: 200rpx
}
.page .contents .product-box .unSelected{
  position: absolute;
  width: 100%;
  height: 80rpx;
  background-color: #666;
  bottom: 0;
  color: #fff;
  padding-left: 20rpx;
  padding-right: 20rpx;
  text-align: center;
  font-size: 0.8rem;
  box-sizing: border-box;
  word-break: break-all;
}
.page .contents .product-box .selected{
  background-color: #009c62;
}